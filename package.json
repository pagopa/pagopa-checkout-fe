{
  "name": "io-pay-portal-frontend",
  "version": "4.33.1",
  "scripts": {
    "type-check": "tsc",
    "generate": "npm-run-all generate:*",
    "generate:payment-ecommerce-v1": "rimraf generated/definitions/payment-ecommerce && shx mkdir -p generated/definitions/payment-ecommerce && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/pagopa-infra/v1.704.0/src/domains/ecommerce-app/api/ecommerce-checkout/v1/_openapi.json.tpl --no-strict --out-dir ./generated/definitions/payment-ecommerce --request-types --response-decoders --client",
    "generate:payment-ecommerce-v2": "rimraf generated/definitions/payment-ecommerce-v2 && shx mkdir -p generated/definitions/payment-ecommerce-v2 && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/pagopa-infra/v1.704.0/src/domains/ecommerce-app/api/ecommerce-checkout/v2/_openapi.json.tpl --no-strict --out-dir ./generated/definitions/payment-ecommerce-v2 --request-types --response-decoders --client",
    "generate:payment-ecommerce-v3": "rimraf generated/definitions/payment-ecommerce-v3 && shx mkdir -p generated/definitions/payment-ecommerce-v3 && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/pagopa-infra/v1.704.0/src/domains/ecommerce-app/api/ecommerce-checkout/v3/_openapi.json.tpl --no-strict --out-dir ./generated/definitions/payment-ecommerce-v3 --request-types --response-decoders --client",
    "generate:payment-ecommerce-v4": "rimraf generated/definitions/payment-ecommerce-v4 && shx mkdir -p generated/definitions/payment-ecommerce-v4 && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/pagopa-infra/v1.704.0/src/domains/ecommerce-app/api/ecommerce-checkout/v4/_openapi.json.tpl --no-strict --out-dir ./generated/definitions/payment-ecommerce-v4 --request-types --response-decoders --client",
    "generate:checkout-auth-service-v1": "rimraf generated/definitions/checkout-auth-service-v1 && shx mkdir -p generated/definitions/checkout-auth-service-v1 && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/pagopa-infra/v1.704.0/src/domains/checkout-app/api/checkout/checkout_auth_service/v1/_openapi.json.tpl --no-strict --out-dir ./generated/definitions/checkout-auth-service-v1 --request-types --response-decoders --client",
    "generate:checkout-feature-flags": "rimraf generated/definitions/checkout-feature-flags && shx mkdir -p generated/definitions/checkout-feature-flags && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/pagopa-infra/v1.704.0/src/domains/checkout-app/api/checkout/checkout_feature_flags/v1/_openapi.json.tpl --no-strict --out-dir ./generated/definitions/checkout-feature-flags --request-types --response-decoders --client",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx",
    "lint-autofix": "eslint . -c .eslintrc.js --ext .ts,.tsx --fix",
    "prebuild": "npm-run-all generate type-check lint",
    "build": "rimraf .cache && rimraf dist && parcel build src/index.html && shx mkdir -p dist/assets && cp -R src/assets/creditcard dist/assets && cp -R src/assets/logos dist/assets && cp -R static/* dist/",
    "dev": "rimraf .cache && rimraf dist && npm-run-all generate type-check dev:env dev:server",
    "proxy": "npm-run-all generate type-check dev:env dev:proxy",
    "dev:env": "export $(grep -v '^#' .env.development | xargs) && chmod +x env.sh && ./env.sh",
    "dev:server": "parcel src/index.html &",
    "dev:proxy": "parcel src/index.html",
    "test": "jest --config=jest.config.js",
    "test:payment-flow-integration": "jest --config=jest.payment-flow.integration.config.js",
    "test:authentication-integration": "jest --config=jest.authentication.integration.config.js",
    "test:outcomes-integration": "jest --config=jest.outcomes.integration.config.js",
    "test:payment-responsepageV2": "jest --config=jest.payment-response-pagev2.integration.config.js",
    "test:feature-flags-integration": "jest --config=jest.feature-flags.integration.config.js",
    "test:outcome-polling-integration": "jest --config=jest.polling.integration.config.js",
    "test:coverage": "jest --coverage --passWithNoTests --config=jest.config.js",
    "preversion": "auto-changelog  --config .auto-changelog.json --unreleased --commit-limit false --stdout --template preview.hbs",
    "version": "auto-changelog -p --config .auto-changelog.json --unreleased && git add CHANGELOG.md",
    "prestart-mock": "export $(grep -v '^#' .env.development | xargs) && chmod +x env.sh && source env.sh",
    "start-mock": "parcel src/index.html & mkdir -p dist && cp -R ot dist/ot"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
