{
  "name": "io-pay-portal-frontend",
  "version": "4.1.0",
  "scripts": {
    "type-check": "tsc",
    "generate": "npm-run-all generate:*",
    "generate:payment-transactions-api": "rimraf generated/definitions/payment-transactions-api && shx mkdir -p generated/definitions/payment-transactions-api && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/pagopa-functions-checkout/v1.1.2/openapi/index.yaml --no-strict --out-dir ./generated/definitions/payment-transactions-api --request-types --response-decoders --client",
    "generate:payment-ecommerce-v1": "rimraf generated/definitions/payment-ecommerce && shx mkdir -p generated/definitions/payment-ecommerce && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/pagopa-infra/v1.200.0/src/domains/ecommerce-app/api/ecommerce-checkout/v1/_openapi.json.tpl --no-strict --out-dir ./generated/definitions/payment-ecommerce --request-types --response-decoders --client",
    "generate:payment-ecommerce-v2": "rimraf generated/definitions/payment-ecommerce-v2 && shx mkdir -p generated/definitions/payment-ecommerce-v2 && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/pagopa-infra/v1.179.0/src/domains/ecommerce-app/api/ecommerce-checkout/v2/_openapi.json.tpl --no-strict --out-dir ./generated/definitions/payment-ecommerce-v2 --request-types --response-decoders --client",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx",
    "lint-autofix": "eslint . -c .eslintrc.js --ext .ts,.tsx --fix",
    "prebuild": "npm-run-all generate type-check lint",
    "build": "rimraf .cache && rimraf dist && parcel build src/index.html && shx mkdir -p dist/assets && cp -R src/assets/creditcard dist/assets && cp -R src/assets/logos dist/assets && cp -R static/* dist/",
    "dev": "rimraf .cache && rimraf dist && npm-run-all generate type-check dev:env dev:server",
    "proxy": "npm-run-all generate type-check dev:env dev:proxy",
    "dev:env": "export $(grep -v '^#' .env.development | xargs) && chmod +x env.sh && ./env.sh",
    "dev:server": "parcel src/index.html &",
    "dev:proxy": "parcel src/index.html",
    "test": "jest --config=jest.config.js",
    "test:final-status-integration": "jest --config=jest.final-status.integration.config.js",
    "test:payment-flow-integration": "jest --config=jest.payment-flow.integration.config.js",
    "test:coverage": "jest --coverage --passWithNoTests --config=package.json",
    "preversion": "auto-changelog  --config .auto-changelog.json --unreleased --commit-limit false --stdout --template preview.hbs",
    "version": "auto-changelog -p --config .auto-changelog.json --unreleased && git add CHANGELOG.md",
    "prestart-mock": "export $(grep -v '^#' .env.development | xargs) && chmod +x env.sh && source env.sh",
    "start-mock": "parcel src/index.html & mkdir -p dist && cp -R ot dist/ot"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/lab": "^5.0.0-alpha.140",
    "@mui/material": "^5.14.5",
    "@pagopa/mui-italia": "1.4.0",
    "@pagopa/openapi-codegen-ts": "^13.2.0",
    "@reduxjs/toolkit": "^1.7.2",
    "@types/dotenv": "^8.2.0",
    "@types/mixpanel-browser": "^2.35.6",
    "abort-controller": "^3.0.0",
    "card-validator": "^8.1.1",
    "formik": "^2.2.9",
    "hex-to-uuid": "^1.1.1",
    "history": "5",
    "i18next": "^21.6.5",
    "i18next-browser-languagedetector": "^6.1.2",
    "lodash": "^4.17.21",
    "mixpanel-browser": "^2.41.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-recaptcha": "^2.1.0",
    "react-i18next": "^11.15.3",
    "react-qr-code": "^2.0.3",
    "react-qr-reader": "^2.2.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "6",
    "set-value": "^4.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "@types/express": "^4.17.9",
    "@types/jest": "^25.2.1",
    "@types/node-fetch": "^2.5.7",
    "@types/puppeteer": "5.4.2",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-google-recaptcha": "^2.1.4",
    "@types/react-qr-reader": "^2.1.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "auto-changelog": "^2.2.1",
    "axios": "^0.26.0",
    "danger": "^10.5.3",
    "danger-plugin-digitalcitizenship": "^1.0.0",
    "dotenv": "^16.0.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-functional": "^3.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-sonarjs": "^0.5.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.6",
    "italia-tslint-rules": "^1.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-puppeteer": "^6.1.0",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.3.2",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "promise-polyfill": "^8.2.0",
    "puppeteer": "^20.0.0",
    "querystring-es3": "^0.2.1",
    "request": "^2.88.2",
    "rimraf": "^2.6.2",
    "sass": "1.27.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.5.0",
    "tslint": "^5.1.0",
    "typescript": "^4.1.2",
    "url": "^0.11.0",
    "whatwg-fetch": "^3.5.0"
  },
  "resolutions": {
    "set-value": "^4.0.1",
    "lodash": "^4.17.21",
    "glob-parent": "^5.1.2",
    "source-map": "^0.8.0-beta.0"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "js",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.ts"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "./test_reports",
          "outputName": "checkout-unit-TEST.xml"
        }
      ]
    ]
  }
}
